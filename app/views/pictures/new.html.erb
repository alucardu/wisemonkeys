<% provide(:title, 'Picture Upload') %>
<h1>Picture Upload</h1>


    <%= form_for @picture do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.submit "Upload Picture", class: "btn btn-large btn-primary" %>
    <% end %>
